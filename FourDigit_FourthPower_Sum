# for i from 1000 to 9999
for i in range(1000, 10000):
  # obtain each digit of the four digit number
  digit1=(i//1000)
  digit2=(i//100%10)
  digit3=(i//10%10)
  digit4=(i%10)

  #print(digit1, digit2, digit3, digit4)

  #calculate sum of digits to the fourth power
  s=digit1**4+digit2**4+digit3**4+digit4**4

  #print(s)

  # test if sum of fourth power digits equals the number
  if s==i:
    # if equal, display the number
    print(f"{i} is the sum of the fourth power of it's digits")
